# Uncomment the next line to define a global platform for your project

source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'
# platform :ios, '9.0'

target 'pokedex3' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'Alamofire', '~> 4.4'

  # Pods for pokedex3

  target 'pokedex3Tests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire', '~> 4.4'
  end

  target 'pokedex3UITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire', '~> 4.4'
  end

end
